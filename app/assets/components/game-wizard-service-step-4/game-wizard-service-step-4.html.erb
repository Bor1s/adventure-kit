<polymer-element name="game-wizard-service-step-4" attributes='game url gameCreated'>
  <template>
    <core-ajax id='dispatcher' url='{{url}}'
      handleAs='json'
      method='POST'
      headers='{"Accept": "application/json"}'
      contentType='application/json'
      on-core-error='{{handleError}}'
      on-core-response='{{handleSuccess}}'>
    </core-ajax>

  </template>
  <script src="game-wizard-service-step-4.js.coffee"></script>
</polymer-element>
