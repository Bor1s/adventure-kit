<polymer-element name="game-service" attributes='games url searchText'>
  <template>

    <core-ajax id='dispatcher' url='{{url}}'
      handleAs='json'
      headers='{"Accept": "application/json"}'
      contentType='application/json'
      params='{"f": "{{optionsToSend}}", "q": "{{searchText}}"}'
      on-core-error='{{handleError}}'
      on-core-response='{{handleSuccess}}'>
    </core-ajax>

    </template>
  <script src="game-service.js.coffee"></script>
</polymer-element>
