<polymer-element name="game-wizard-service-step-1" attributes='game url'>
  <template>

    <core-ajax id='dispatcher' url='{{url}}'
      handleAs='json'
      method='POST'
      headers='{"Accept": "application/json"}'
      contentType='application/json'
      on-core-error='{{handleError}}'
      on-core-response='{{handleSuccess}}'>
    </core-ajax>

    </template>
  <script src="game-wizard-service-step-1.js.coffee"></script>
</polymer-element>
